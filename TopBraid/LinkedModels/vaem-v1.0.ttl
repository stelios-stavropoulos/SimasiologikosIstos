  rdfs:comment "A domain is a subject area of interest. Examples of domains are \"enterprise\", \"process\", \"organization\" and \"system\". In VAEM, \"Domain\" is used as a dimension for characterizing an ontology graph." ;
  rdfs:isDefinedBy <http://www.linkedmodel.org/1.0/schema/vaem> ;
  rdfs:label "Domain" ;
