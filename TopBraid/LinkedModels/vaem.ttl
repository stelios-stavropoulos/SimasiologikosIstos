.
vaem:publishedDate
  a owl:DatatypeProperty ;
  dct:description "The property \"vaem:publishedDate\" specifies the date and time that the ontology was published. This is distinct from a release date in the sense that \"release\" may not imply being published."^^rdf:HTML ;
  rdfs:isDefinedBy <http://www.linkedmodel.org/schema/vaem> ;
  rdfs:label "published date" ;
  rdfs:range xsd:date ;
  rdfs:seeAlso vaem:releaseDate ;
  rdfs:subPropertyOf vaem:date ;
.
vaem:rationale
  a owl:DatatypeProperty ;
  dct:description "A description of a justification for some other assertion or value"^^rdf:HTML ;
  rdfs:isDefinedBy <http://www.linkedmodel.org/schema/vaem> ;
  rdfs:label "rationale" ;
  rdfs:range xsd:string ;
.
vaem:rdfxmlFileURL
  a owl:DatatypeProperty ;
  dct:description "The property \"vaem:rdfxmlFileURL\" is used to provide a link to the location of the ontology's RDF/XML file"^^rdf:HTML ;
  rdfs:isDefinedBy <http://www.linkedmodel.org/schema/vaem> ;
