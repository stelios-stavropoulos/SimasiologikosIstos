  rdfs:isDefinedBy <http://www.linkedmodel.org/schema/vaem> ;
  rdfs:label "description" ;
.
vaem:filePrefix
  a owl:DatatypeProperty ;
  dct:description "The property \"vaem:filePrefix\" is for specifying a preferred file prefix for a graph. For example a schema graph may be given the file prefix \"SCHEMA\" followed by an underscore \"_\"."^^rdf:HTML ;
  rdfs:isDefinedBy <http://www.linkedmodel.org/schema/vaem> ;
  rdfs:label "file prefix" ;
  rdfs:range xsd:string ;
.
