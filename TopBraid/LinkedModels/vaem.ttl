  rdfs:isDefinedBy <http://www.linkedmodel.org/schema/vaem> ;
  rdfs:label "is elaborated in" ;
  rdfs:range xsd:anyURI ;
  rdfs:subPropertyOf rdfs:isDefinedBy ;
.
vaem:isMetadataFor
  a owl:AnnotationProperty ;
  dct:description "An annotation used to reference the graph that is being described with metadata."^^rdf:HTML ;
  rdfs:isDefinedBy <http://www.linkedmodel.org/schema/vaem> ;
  rdfs:label "is metadata for" ;
.
vaem:lastUpdated
  dct:description "The property \"vaem:lastUpdated\" is intended for general use as the name implies.  The range of the property is set as \"vaem:dateUnion\" so as to allow a value to have a number of forms from a  year only to a full timestamp"^^rdf:HTML ;
.
vaem:latestPublishedVersion
  a owl:DatatypeProperty ;
