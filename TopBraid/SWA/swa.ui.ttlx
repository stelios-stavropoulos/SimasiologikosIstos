swa:fullTextSearchPropertiesConcat
  a spin:Function ;
  spin:body [
      a sp:Select ;
      sp:orderBy (
          [
            sp:varName "property" ;
          ]
        ) ;
      sp:resultVariables (
          [
            sp:expression [
                a sp:GroupConcat ;
                sp:expression [
                    sp:varName "str" ;
                  ] ;
              ] ;
            sp:varName "result" ;
          ]
        ) ;
      sp:where (
          [
            a sp:TriplePath ;
            sp:object [
                sp:varName "type" ;
              ] ;
            sp:path [
                a sp:ModPath ;
                sp:modMax -2 ;
                sp:modMin 0 ;
                sp:subPath rdfs:subClassOf ;
              ] ;
            sp:subject [
                sp:varName "subClass" ;
              ] ;
          ]
          [
            a sp:NamedGraph ;
            sp:elements (
                [
                  sp:object [
                      sp:varName "property" ;
                    ] ;
                  sp:predicate swa:fullTextSearchProperty ;
                  sp:subject [
                      sp:varName "type" ;
                    ] ;
                ]
              ) ;
            sp:graphNameNode ui:graph ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a sp:concat ;
                sp:arg1 "<" ;
                sp:arg2 [
                    a xsd:string ;
                    sp:arg1 [
                        sp:varName "property" ;
                      ] ;
                  ] ;
                sp:arg3 "> " ;
              ] ;
            sp:variable [
                sp:varName "str" ;
              ] ;
          ]
        ) ;
    ] ;
  spin:private true ;
  spin:returnType xsd:string ;
  rdfs:label "full text search properties concat" ;
  rdfs:subClassOf swa:FullTextSearchFunctions ;
