                    <swa:Space/>
                </ui:if>
                <button class=\"btn btn-light btn-xs save-button\" data-formid=\"{= ?fid }\" id=\"form-save-button-{= ?fid }\" onclick=\"swa.submitForm('{= ?fid }', null)\">
