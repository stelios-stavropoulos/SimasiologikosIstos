swa:bestCreateResourceDialogPlugin
  a spin:Function ;
  spin:body [
      a sp:Select ;
      sp:resultVariables (
          [
            sp:varName "plugin" ;
          ]
        ) ;
      sp:where (
          (
            [
              a sp:NamedGraph ;
              sp:elements (
                  [
                    a sp:SubQuery ;
                    sp:query [
                        a sp:Select ;
                        sp:orderBy (
                            [
                              a sp:Desc ;
                              sp:expression [
                                  sp:varName "weight" ;
                                ] ;
                            ]
                          ) ;
                        sp:where (
                            [
                              sp:object [
                                  sp:varName "w" ;
                                ] ;
                              sp:predicate swa:createResourceDialogPluginWeight ;
                              sp:subject [
                                  sp:varName "plugin" ;
                                ] ;
                            ]
                            [
                              sp:object [
                                  sp:varName "expression" ;
                                ] ;
                              sp:predicate swa:expression ;
                              sp:subject [
                                  sp:varName "w" ;
                                ] ;
                            ]
                            [
                              a sp:Bind ;
                              sp:expression [
                                  a sp:coalesce ;
                                  sp:arg1 [
                                      a spl:object ;
                                      sp:arg1 [
                                          sp:varName "w" ;
                                        ] ;
                                      sp:arg2 swa:weight ;
                                    ] ;
                                  sp:arg2 0 ;
                                ] ;
                              sp:variable [
                                  sp:varName "weight" ;
                                ] ;
                            ]
                          ) ;
                      ] ;
                  ]
                  [
                    a sp:Filter ;
                    sp:expression [
                        a spin:eval ;
                        sp:arg1 [
                            sp:varName "expression" ;
                          ] ;
                        sp:arg2 arg:resourceType ;
                        sp:arg3 [
                            sp:varName "resourceType" ;
                          ] ;
                        sp:arg4 arg:contextResource ;
                        sp:arg5 [
                            sp:varName "contextResource" ;
                          ] ;
                        sp:arg6 arg:appName ;
                        sp:arg7 [
                            a sp:coalesce ;
                            sp:arg1 [
                                a swa:appName ;
                              ] ;
                            sp:arg2 "NONE" ;
                          ] ;
                      ] ;
                  ]
                ) ;
              sp:graphNameNode ui:graph ;
            ]
          )
        ) ;
    ] ;
  spin:constraint [
      a spl:Argument ;
      spl:optional true ;
      spl:predicate arg:contextResource ;
      spl:valueType rdfs:Resource ;
      rdfs:comment "The context resource." ;
    ] ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate arg:resourceType ;
      spl:valueType rdfs:Class ;
      rdfs:comment "The resource type." ;
    ] ;
  spin:private true ;
  spin:returnType swa:CreateResourceDialogPlugin ;
  rdfs:label "best create resource dialog plugin" ;
  rdfs:subClassOf swa:Functions ;
