                        ?thisParent swa:used ?predicate .
                    } .
                } .
                FILTER (!swa:isHiddenPropertyAtSubject(?predicate, ?subject)) .
                FILTER ((!bound(?filterFunction)) || spif:invoke(?filterFunction, ?predicate, ?subject)) .
