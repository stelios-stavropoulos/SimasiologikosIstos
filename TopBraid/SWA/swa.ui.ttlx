<ui:group let:classPrefix=\"{= swa:getURIClassPrefix() }\" let:constructMethod=\"{= swa:getURIConstructMethod() }\" let:counterOffset=\"{= swa:getURICounterOffset(?resourceType) }\" let:teamGraph=\"{= teamwork:currentTeamGraph() }\">
