swa:DefaultDeleteResourcesService
  a swa:DeleteResourcesService ;
  ui:prototype """
<ui:group letrs:rs='{#
        SELECT ?resource
        WHERE {
            ?str spif:split ( ?resources \" \" ) .
            BIND (ui:decodeNode(CONCAT(\"&lt;\", ?str, \"&gt;\")) AS ?resource) .
        } }'>
    <ui:forEach ui:resultSet=\"{= ?rs }\">
