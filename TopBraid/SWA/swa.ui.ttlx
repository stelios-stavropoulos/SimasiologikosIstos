        <ui:else>
            <script>$(\"#{= ?fid }-loadable\").removeClass('swa-form-edit')</script>
            <ui:setContext ui:varName=\"mode\" ui:varValue=\"{= COALESCE(?viewModeName, &quot;view&quot;) }\">
