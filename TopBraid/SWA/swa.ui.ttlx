                                  ] ;
                                sp:arg2 [
                                    sp:varName "actionAppName" ;
                                  ] ;
                              ] ;
                          ] ;
                        sp:arg3 [
                            a sp:or ;
                            sp:arg1 [
                                a sp:not ;
                                sp:arg1 [
                                    a sp:bound ;
                                    sp:arg1 [
                                        sp:varName "appName" ;
